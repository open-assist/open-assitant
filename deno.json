{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable-kv -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "debug": "deno run -A --inspect-wait --unstable-kv --watch=static/,routes/ dev.ts",
    "debug:link": "deno run -A --inspect-wait --import-map=link.json --unstable-kv --watch=static/,routes/ dev.ts",
    "start": "deno run -A --unstable-kv --watch=static/,routes/ dev.ts",
    "start:link": "deno run -A --import-map=link.json --unstable-kv --watch=static/,routes/ dev.ts",
    "build": "deno run -A --unstable-kv dev.ts build",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"]
    }
  },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "$/": "./",
    "$fresh/": "https://deno.land/x/fresh@1.6.5/",
    "$std/": "https://deno.land/std@0.219.0/",
    "zod": "https://deno.land/x/zod@v3.22.4/mod.ts",
    "openai_schemas": "https://deno.land/x/open_schemas@1.3.0/zod/openai/mod.ts",
    "ollama_schemas": "https://deno.land/x/open_schemas@1.3.0/zod/ollama/mod.ts",
    "anthropic_schemas": "https://deno.land/x/open_schemas@1.3.0/zod/anthropic/mod.ts",
    "googleai_schemas": "https://deno.land/x/open_schemas@1.3.0/zod/googleai/mod.ts",
    "fast-xml-parser": "https://esm.sh/fast-xml-parser@4.3.6",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/"
  },
  "nodeModulesDir": true
}
